#: ../../userguides/audio.rst:91
msgid ":guilabel:`Spectrometer`"
msgstr ""

# c06ed2ec80c24d8cbd533607d6e49cf5
#: ../../userguides/audio.rst:92
msgid ":guilabel:`Scope`"
msgstr ""

# 0baa0eeaec9a48ccaa3dc43b905e8fab
#: ../../userguides/audio.rst:93
msgid ":guilabel:`Spectrum`"
msgstr ""

# 418abec09d64414fa8fd0f65debb7c38
#: ../../userguides/audio.rst:94
msgid ":guilabel:`VU Meter`"
msgstr ""

# 99de3080c24048409227d549b3a21e88
#: ../../userguides/audio.rst:95
msgid ":guilabel:`Goom`"
msgstr ""

# b5afe181fd3b4164b2d15efa33670333
#: ../../userguides/audio.rst:96
msgid ":guilabel:`projectM`"
msgstr ""

# 5ad2a18b4e1049e4a198a5f5a40d2d93
#: ../../userguides/audio.rst:97
msgid ":guilabel:`3D Spectrum`"
msgstr ""

# e131b9cafbad4bb6befb14499b9a6aab
#: ../../userguides/audio.rst:102
msgid ""
"To disable visualizations, click on :menuselection:`Visualizations --> "
"Disable` drop down menu."
msgstr ""

# c8d03cb86fa74927889b3622e6c5d3d7
#: ../../userguides/audio.rst:106
msgid "Maximum VLC Volume"
msgstr ""

# 6780cfbcef80449390d494d72509141f
#: ../../userguides/audio.rst:108
msgid ""
"To change the maximum volume in of an audio file, go to "
":menuselection:`Tools --> Preferences`, select :guilabel:`All` at bottom "
"left corner, then :menuselection:`Interface --> Main interface --> Qt -->"
" Maximum volume displayed`. Then :guilabel:`Save` it and restart VLC."
msgstr ""

